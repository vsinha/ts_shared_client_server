{
  "name": "Game",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=6.9.5"
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx'",
    "start": "node ./dist/server/server.js",
    "start:prod": "cross-env NODE_ENV=production node ./dist/server/server.js",
    "build-client": "cd ./src/client && cross-env NODE_ENV=production webpack -p",
    "dev-client": "cd ./src/client && webpack-dev-server -w",
    "build-server": "tsc -p ./src/server",
    "dev-server": "tsc-watch -p ./src/server --onSuccess \"node ./dist/server/server.js\"",
    "dev": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run dev-server\" \"npm run dev-client\"",
    "build": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run build-server\" \"npm run build-client\"",
    "test": "jest"
  },
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "typescript",
    "react",
    "starter-kit",
    "webpack",
    "fullstack",
    "express",
    "express4",
    "node"
  ],
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./src/server/tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  },
  "license": "MIT",
  "author": "Viraj Sinha",
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/jest": "^21.1.5",
    "@types/node": "^7.0.11",
    "@types/react-router-dom": "^4.0.1",
    "@types/uuid": "^3.4.3",
    "awesome-typescript-loader": "^3.1.2",
    "babel-polyfill": "^6.23.0",
    "concurrently": "^3.4.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^21.2.1",
    "open-browser-webpack-plugin": "0.0.5",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.1",
    "style-loader": "^0.16.1",
    "ts-jest": "^21.1.4",
    "tsc-watch": "^1.0.5",
    "tslint": "^5.1.0",
    "typescript": "^2.2.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "body-parser": "^1.17.1",
    "bootstrap": "^3.3.7",
    "concurrently": "^3.4.0",
    "express": "^4.15.2",
    "file-loader": "^0.11.1",
    "http-proxy-middleware": "^0.17.4",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.10",
    "react-dom": "^15.4.2",
    "react-router-bootstrap": "^0.24.2",
    "react-router-dom": "^4.0.0",
    "tslib": "^1.6.0",
    "url-loader": "^0.5.8",
    "uuid": "^3.1.0",
    "ws": "^3.2.0"
  }
}
